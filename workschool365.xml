      <Resource>api://localhost:3000/72debd24-da37-426b-906d-86063ee910b8</Resource>
-      <Scopes>
        <Scope>User.Read</Scope>
        <Scope>profile</Scope>
      </Scopes>
    </WebApplicationInfo>

  </VersionOverrides>
</OfficeApp>